name: Analisis Punto X
on:
  workflow_dispatch:
  push:
    branches:
      - main

jobs:
  configuracion:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout
        uses: actions/checkout@v4
      - name: Validar Punto Cero CIP 1.0000
        run: echo "Iniciando calibracion desde el Punto X"